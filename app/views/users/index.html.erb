<div class="row">
  <h1>VERSUS</h1>
</div>
<div class="row">
  <%= render :partial => 'users/carousel' %>
</div>
<div class="row">
  <h1>운명을 예측해 보세요.</h1>
</div>
<div class="row">
  <div class="col-xs-4">
    <%=link_to "/auth/facebook" do%>
      <button type="button" class="btn btn-primary">페이스북</button>
    <%end%>
  </div>
  <div class="col-xs-4">
    <%=link_to "/auth/facebook" do%>
      <button type="button" class="btn btn-primary">트위터</button>
    <%end%>
  </div>
  <div class="col-xs-4">
    <button type="button" class="btn btn-primary" data-toggle="modal" data-target="#reg-nick">게스트</button>
  </div>
</div>
<div class="modal fade" id="reg-nick" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
        <h4 class="modal-title" id="myModalLabel">Modal title</h4>
      </div>
      <div class="modal-body">
        ...
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
        <button type="button" class="btn btn-primary">Save changes</button>
      </div>
    </div>
  </div>
</div>
