<div class="modal fade" id="if-signup" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
        <h4 class="modal-title" id="myModalLabel">IF 가입</h4>
      </div>
      <div class="modal-body">
        <div class="input-group">
          <input id="vs-email" type="text" class="form-control" placeholder="이메일">
          <input id="vs-pwd" type="password" class="form-control" placeholder="비밀번호">
          <input id="vs-nick" type="text" class="form-control" placeholder="닉네임">
        </div>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-primary">Join</button>
        <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
      </div>
    </div>
  </div>
</div>

<div class="modal fade" id="if-signin" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
        <h4 class="modal-title" id="myModalLabel">IF 로그인</h4>
      </div>
      <div class="modal-body">
        <div class="input-group">
          <input id="vs-email" type="text" class="form-control" placeholder="이메일">
          <input id="vs-pwd" type="password" class="form-control" placeholder="비밀번호">
        </div>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-primary">Login</button>
        <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
      </div>
    </div>
  </div>
</div>
